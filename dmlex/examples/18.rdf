PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dmlex: <https://www.oasis-open.org/to-be-confirmed/dmlex#>

[] a dmlex:LexicographicResource;
  dmlex:entry <file:the-bible>,
    <file:bible>;
  dmlex:relation [ a dmlex:Relation;
      dmlex:type "subentrying";
      dmlex:member [ a dmlex:Member;
          dmlex:role "subentry";
          dmlex:obverseListingOrder "0"^^xsd:unsignedInt;
          dmlex:listingOrder "2"^^xsd:unsignedInt;
          dmlex:ref "the-bible" ],
        [ a dmlex:Member;
          dmlex:role "container";
          dmlex:ref "bible-1";
          dmlex:listingOrder "1"^^xsd:unsignedInt;
          dmlex:obverseListingOrder "0"^^xsd:unsignedInt ];
      dmlex:listingOrder "1"^^xsd:unsignedInt ];
  dmlex:uri "http://example.com";
  dmlex:langCode "en".

<file:bible-2> a dmlex:Sense;
  dmlex:listingOrder "2"^^xsd:unsignedInt;
  dmlex:definition [ a dmlex:Definition;
      dmlex:listingOrder "1"^^xsd:unsignedInt;
      dmlex:text "a book considered important for a subject" ].

<file:the-bible> a dmlex:Entry;
  dmlex:headword "the Bible";
  dmlex:sense <file:the-bible-1>.

<file:the-bible-1> a dmlex:Sense;
  dmlex:definition [ a dmlex:Definition;
      dmlex:text "the book considered holy by Christians";
      dmlex:listingOrder "1"^^xsd:unsignedInt ];
  dmlex:listingOrder "1"^^xsd:unsignedInt.

<file:bible-1> a dmlex:Sense;
  dmlex:listingOrder "1"^^xsd:unsignedInt.

<file:bible> a dmlex:Entry;
  dmlex:headword "bible";
  dmlex:sense <file:bible-1>,
    <file:bible-2>.

